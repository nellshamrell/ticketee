<%= form_for [@project, @ticket] do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>
  <% number = 0 %>
  <div id='files'>
    <%= render :partial => "files/form",
               :locals => { :number => number } %>
    <%= link_to "Add another file", 'javascript:', :id => "add_another_file" %>
  </div>
  <%= f.submit %>
<% end %>

<h2> <%= @project.name %> </h2>

<% title(@project.name, "Projects") %>

<title>
  <% if content_for?(:title) %>
    <%= yield(:title) %>
  <% else %>
    Ticketee
  <% end %>
</title>

<%= link_to "Edit Project", edit_project_path(@project) %>
<%= link_to "Delete Project", @project, :method => :delete, :confirm => "Are you sure you want to delete this project?"  %>



